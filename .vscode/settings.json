// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  
  "go.buildTags": "json",
  "go.lintTool": "gometalinter",
  "go.formatTool": "goimports",

  "protoc": {
    "options": [
      "--proto_path=$GOPATH/src/",
      "--proto_path=$GOPATH/src/github.com/google/protobuf/src"
    ]
  }
}